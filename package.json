{
  "name": "rhlf-test-project",
  "version": "1.0.0",
  "description": "RESTful API in Node.js for an AddressBook",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "dev": "nodemon -w src --exec \"babel-node src --presets env\"",
    "build": "rimraf dist && babel src -s -D -d dist --presets env",
    "clean": "rm -rf build && mkdir build",
    "start": "node dist"
  },
  "engines": {
    "node": "10.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://renatohlf@bitbucket.org/renatohlf/rhlf-test-project.git"
  },
  "author": "Renato H. L. Farias",
  "license": "ISC",
  "homepage": "https://bitbucket.org/renatohlf/rhlf-test-project#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "consign": "^0.1.6",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "eslint": "^5.5.0",
    "express": "^4.16.3",
    "express-pino-logger": "^4.0.0",
    "express-validator": "^5.2.0",
    "firebase": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "rimraf": "^2.6.2",
    "sequelize": "^4.38.1",
    "superagent": "^4.0.0-beta.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "sinon": "^6.3.4",
    "supertest": "^3.1.0"
  }
}
